<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="product-bundle" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Ulmod_BundleItem/js/image" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeUlmodBundleItem/js/image</item>
                            </item>
                            <item name="Ulmod_BundleItem/js/popup" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeUlmodBundleItem/js/popup</item>
                            </item>
                            <item name="Ulmod_BundleItem/js/expand-collapse" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeUlmodBundleItem/js/expand-collapse</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
